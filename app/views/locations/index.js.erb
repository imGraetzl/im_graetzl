(function() {
  var $cardContainer = $("[data-behavior='locations-card-container']");
  var $paginateLink = $("[data-behavior='locations-paginate-link']");


  <% if params[:special_category_id] == 'menus' %>
    <% if params[:page].blank? %>
      $cardContainer.html("<%= j render partial: 'menu', collection: @locations, as: 'location' %>")
    <% else %>
      $cardContainer.append("<%= j render partial: 'menu', collection: @locations, as: 'location' %>");
    <% end %>
  <% else %>
    <% if params[:page].blank? %>
      $cardContainer.html("<%= j render @locations %>")
    <% else %>
      $cardContainer.append("<%= j render @locations %>");
    <% end %>
  <% end %>




  <% if @locations.next_page.present? %>
    $paginateLink.attr('href', "<%= raw(next_page_url(@locations)) %>").removeClass("hide");
  <% else %>
    $paginateLink.addClass("hide");
  <% end %>
})();
