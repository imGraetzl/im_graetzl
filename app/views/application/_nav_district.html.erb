<div class="mainNav">
  <%= link_to 'Home', root_path, class: 'homelink' %>
  <div class="wienTrigger dropdownTrigger" data-horizontal-offset="0" data-mobileNavID="wien">
    <%= icon_tag "arrow-down-2" %>
    <span class="txt">Wien</span>
  </div>
  <div class="graetzlTrigger dropdownTrigger" data-horizontal-offset="0" data-mobileNavID="graetzl">
    <%= icon_tag "arrow-down-2" %>
    <span class="txt"><%= district.name %></span>
  </div>


  <!-- dropDowns -->
  <div class="wienContainer jq-dropdown jq-dropdown-tip jq-dropdown-relative jq-dropdown-anchor-left dropdown-container">
    <div class="jq-dropdown-panel nav-wien">
      <%= link_to wien_path do %>
        <%= icon_tag "map-location" %> Wien Karte
      <% end %>
      <%= link_to rooms_wien_path do %>
        <%= icon_tag "raumteiler" %> Raumteiler
      <% end %>
      <%= link_to tool_offers_wien_path do %>
        <%= icon_tag "tacker" %> Toolteiler
      <% end %>
      <%= link_to groups_wien_path do %>
        <%= icon_tag "coop-share-white" %> Gruppen
      <% end %>
      <%= link_to locations_wien_path do %>
        <%= icon_tag "location" %> Anbieter & Locations
      <% end %>
      <%= link_to meetings_wien_path do %>
        <%= icon_tag "calendar-2" %> Events & Workshops
      <% end %>
      <%= link_to platform_meetings_wien_path do %>
        <%= icon_tag "users" %> Community Treffen
      <% end %>
      <%= link_to zuckerls_wien_path do %>
        <%= icon_tag "shopping-bag" %> Zuckerl & Angebote
      <% end %>
      <%= link_to info_path do %>
        <%= icon_tag "lightbulb" %> Alle Services
      <% end %>
      <!--
      <%= link_to unterstuetzer_team_path do %>
        <%= icon_tag "check" %> Unterst√ºtzer Team
      <% end %>
      -->
      <%= link_to "https://blog.imgraetzl.at/", class: 'discover', target: '_blank' do %>
        <%= icon_tag "coffee" %> Blog
      <% end %>

    </div>
  </div>


  <!-- dropDown -->
  <div class="graetzlContainer jq-dropdown jq-dropdown-tip jq-dropdown-relative jq-dropdown-anchor-left dropdown-container">
    <div class="jq-dropdown-panel nav-graetzl">
      <%= link_to district_path(district) do %>
        <%= icon_tag "home" %> <%= district.name %> (<%= district.numeric %>. Bez.)
      <% end %>
      <%= link_to rooms_district_path(district) do %>
        <%= icon_tag "raumteiler" %> Raumteiler (<%= district.numeric %>. Bez.)
      <% end %>
      <%= link_to tool_offers_district_path(district) do %>
        <%= icon_tag "tacker" %> Toolteiler (<%= district.numeric %>. Bez.)
      <% end %>
      <%= link_to groups_district_path(district) do %>
        <%= icon_tag "coop-share" %> Gruppen (<%= district.numeric %>. Bez.)
      <% end %>
      <%= link_to locations_district_path(district) do %>
        <%= icon_tag "location" %> Anbieter & Locations (<%= district.numeric %>. Bez.)
      <% end %>
      <%= link_to meetings_district_path(district) do %>
        <%= icon_tag "calendar-2" %> Events & Workshops (<%= district.numeric %>. Bez.)
      <% end %>
      <%= link_to zuckerls_district_path(district) do %>
        <%= icon_tag "shopping-bag" %> Zuckerl & Angebote (<%= district.numeric %>. Bez.)
      <% end %>
    </div>
  </div>
</div>
