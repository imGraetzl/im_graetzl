<% @group.last_active_members(21 - @group.admins.size).each do |user| %>
  <div class="user">
    <div class="avatar">
      <%= link_to user_path(user) do %>
        <%= avatar_image_with_tooltip(user, size: :large, class: 'img-round') %>
      <% end %>
    </div>
    <div class="infos">
      <span><%= user.first_name %><br><%= user.last_name %></span>
    </div>
  </div>
<% end %>
