        <div class="input-radio">
          <input required="required" type="radio" value="<%= request_join_group_path(group1) %>" checked="checked" name="group" id="group1">
          <label for="group1">Unterstützer Team</label>
        </div>
        <!--
        <div class="input-radio">
          <input required="required" type="radio" value="<%= request_join_group_path(group1) %>" name="group" id="group2">
          <label for="group2">Unterstützer Team mit Arbeitsplatz</label>
        </div>
        -->
      </div>

      <div class="group-info group1">
        <ul>
          <li><%= icon_tag "check" %>3 Monate Teilnahme am Unterstützer-Programm</li>
        </ul>
        <div class="price">
          <span class="sum">Gesamt: 300,00 €</span>
          <!--<strong class="early">Early Bird Preis bis 08. Jan 2020</strong><br>-->
          <small>(inkl. 20% Mwst)</small>
        </div>
      </div>

      <div class="group-info group2">
        <ul>
          <li><%= icon_tag "check" %>3 Monate Teilnahme am Unterstützer-Programm</li>
          <li><%= icon_tag "check" %>3 Monate Flex-Arbeitsplatz im Coworking Space "Die Tischlerei"</li>
          <li><%= icon_tag "check" %>inkl. Internet, Druckernutzung, gratis Kaffee, Tee, Wasser und 24/7 Zugang</li>
        </ul>
        <div class="price">
          <span class="sum">Gesamt: 948,00 €</span>
          <!--<strong class="early">Early Bird Preis bis 08. Jan 2020</strong><br>-->
          <small>(inkl. 20% Mwst)</small>
        </div>
      </div>

    </div>

    <p class="subline">
      Und noch ein paar Fragen zu deinem Vorhaben:
    </p>

    <div class="anmeldung cardBox <%= '-login login-panel-opener' if !current_user %>" >

      <%= form_tag request_join_group_path(group1), id: "groupform" do %>
        <% if group1.group_join_questions.present? %>

          <% group1.group_join_questions.each do |join_question| %>
            <h4><%= join_question.question %></h4>
            <div class="input-textarea">
              <%= text_area_tag :"join_answers[]", nil, cols: 40, rows: 3, placeholder: 'Deine Antwort ..', required: true %>
            </div>
          <% end %>

        <% else %>

          <div class="input-textarea">
            <%= text_area_tag :request_message, nil, cols: 40, rows: 3, placeholder: 'Schreib noch einen kurzen Satz über Dich, bzw. warum Du beitreten möchtest ..', required: true %>
          </div>

        <% end %>
        <%= submit_tag "Anmeldung abschicken", class: 'btn-primary -large' %>
      <% end %>

    </div>
  <% else %>
    <div class="anmeldung cardBox">
      Keine Gruppe verknüpft ...
    </div>
  <% end %>
  <!-- END TEILNEHMEN -->



