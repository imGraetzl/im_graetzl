      <h3 class="sidebar">Jetzt anfragen</h3>

      <div class="request">
        <%= form_for 'toolteiler' do |f| %>

          <!-- hidden input for removing focus from first date picker - otherwise it would open-->
          <%= f.text_field :hidden, class: 'hidden' %>
          <!-- end hack -->

          <div class="input-field">
            <%= f.label :starts_at_date do %>
              <%= icon_tag "calendar-2" %>
              <span></span>
            <% end %>
            <%= f.text_field :starts_at_date, class: 'starts_at_date', placeholder: 'Von' %>
          </div>

          <div class="input-field">
            <%= f.label :ends_at_date do %>
              <%= icon_tag "calendar-2" %>
              <span></span>
            <% end %>
            <%= f.text_field :ends_at_date, class: 'ends_at_date', placeholder: 'Bis' %>
          </div>

        <% end %>
      </div>

      <h3 class="sidebar">Deine Auswahl</h3>

      <ul class="pricing-summary">
        <li>
          <div class="cost">250,00 €</div>
          <div class="costname">10 Tage x 25 €</div>
        </li>
        <li>
          <div class="cost">- 50,00 €</div>
          <div class="costname">Rabatt 10 Tage x 5 €</div>
        </li>
        <li>
          <div class="cost">40,00 €</div>
          <div class="costname">Servicegebühr inkl. Versicherung</div>
        </li>
        <li class=sum>
          <div class="cost">240,00 €</div>
          <div class="costname">Gesamt</div>
        </li>
