<div class="cardbox-wrp" data-behavior="masonry-card">
  <div class="paperCard -cord1RT -illuteaser -room-boosters">
    <div class="wrp">
      <div class="user">
        <% if current_user %>
          <%= avatar_image current_user, size: :thumb, class: 'avatar img-round show-badge' %>
          <span><%= current_user.username %></span>
        <% else %>
          <picture>
            <source srcset="<%= image_path('fallbacks/user_avatar.webp') %>" type="image/webp" />
            <%= image_tag('fallbacks/user_avatar.png', loading: 'lazy', class: 'img-round') %>
          </picture>
          <span>Dein Name</span>
        <% end %>
      </div>
      <h3>Eine Woche lang, die volle Sichtbarkeit f√ºr deinen Raumteiler.</h3>
      <%= link_to 'Jetzt Raumteiler pushen', new_room_booster_path, class: 'btn-primary' %>
    </div>
    <div class="card"></div>
  </div>
</div>
