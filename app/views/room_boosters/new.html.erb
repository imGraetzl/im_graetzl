<% meta(
    title: t('.title'),
    robots: 'noindex, nofollow'
  )
%>

<section class="room-booster-page address-screen">

  <div class="col-content">
    <%= render "room_boosters/new/tabs_nav" %>
    <% if @room_booster.paused? %>
      <%= render 'room_boosters/new/address_disabled' %>
    <% else %>
      <%= render 'room_boosters/new/address' %>
    <% end %>    
  </div>

  <div class="col-sidebar">
    <%= render @room_offer %>
    <%= render 'room_boosters/new/key_data' %>
  </div>

</section>
