<div class="cardbox-wrp" data-behavior="masonry-card">
  <div class="paperCard -cord1RT -illuteaser -location">
    <div class="wrp">
      <div class="user">
        <% if current_user %>
          <%= avatar_image current_user, size: :thumb, class: 'avatar img-round' %>
          <span><%= current_user.username %></span>
        <% else %>
          <%= image_tag('fallbacks/user_avatar.png', class: 'img-round') %>
          <span>Dein Name</span>
        <% end %>
      </div>
      <h3>Noch nicht auf <%= t("region.#{current_region.id}.claim_readable") %> registriert? Melde dich gleich <%= t("region.#{current_region.id}.in_your_graetzl") %> an.</h3>
      <%= link_to "Hier kostenlos registrieren", new_user_session_path, class: 'btn-primary -rose' %>
    </div>
    <div class="card"></div>
  </div>
</div>
